<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="com.wyb.rec.domain.User" table="user">
		<!-- 主键 -->
		<id name="idUser" column="idUser">
			<generator class="native"></generator>
		</id>
		<!-- 普通 -->
		<property name="userName"  column="userName" ></property>
		<property name="userEmail"  column="userEmail" ></property>
		<property name="userPass"  column="userPass" ></property>
		<property name="registerDate"  column="registerDate" ></property>
		<property name="userState" column="userState"></property>
		
	</class>
</hibernate-mapping>