<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="com.wyb.rec.domain.UserAndSlType" table="userandsltype">
		<!-- 主键 -->
		<id name="iduserandsltype" column="iduserandsltype">
			<generator class="native"></generator>
		</id>
		<!-- 普通 -->
		<property name="idUser"  column="idUser" ></property>
		<property name="idtype"  column="idtype" ></property>
		<property name="times"  column="times" ></property>
		
		
	</class>
</hibernate-mapping>